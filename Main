
public class Main {

    public static void main(String[] args) {
        Follower follower1 = new Follower();
        Follower follower2 = new Follower();

        FootballGroup group = new FootballGroup();
        group.follow(follower1);
        group.follow(follower2);

        group.setnews("Man U won Man C");

        group.unfollow(follower2);

        group.setnews("Man U won EPL");
    }
}
